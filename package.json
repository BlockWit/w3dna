{
  "name": "bns-contracts",
  "version": "1.0.0",
  "description": "BNS Smart contracts",
  "scripts": {
    "md-pdf-manager": "cd ./docs && sed -r 's/<details>/<details open>/; s/<summary.+summary>//' manager.md > manager_print.md && md-to-pdf manager_print.md && mv manager_print.pdf manager.pdf && rm manager_print.md",
    "md-pdf-user": "cd ./docs && sed -r 's/<details>/<details open>/; s/<summary.+summary>//' user.md > user_print.md && md-to-pdf user_print.md && mv user_print.pdf user.pdf && rm user_print.md",
    "md-pdf": "npm run md-pdf-manager && npm run md-pdf-user"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BlockWit/bns-contracts"
  },
  "author": "BlockWit",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BlockWit/bns-contracts"
  },
  "homepage": "https://github.com/BlockWit/bns-contracts#readme",
  "devDependencies": {
    "@metamask/detect-provider": "^1.2.0",
    "@openzeppelin/cli": "^2.8.2",
    "@openzeppelin/contracts": "^4.2.0",
    "@openzeppelin/test-environment": "^0.1.9",
    "@openzeppelin/test-helpers": "^0.5.12",
    "@truffle/hdwallet-provider": "^1.4.3",
    "babel-loader": "^8.2.2",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-mocha-no-only": "^1.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-standard": "^5.0.0",
    "ethers": "^5.4.6",
    "md-to-pdf": "^4.0.0",
    "mocha": "^9.0.3",
    "solhint": "^3.3.7",
    "truffle": "^5.5.18",
    "truffle-plugin-verify": "^0.5.26",
    "web3": "^1.7.3",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0"
  }
}
